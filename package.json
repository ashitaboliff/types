		"check:fix": "biome check --write .",
		"prepublishOnly": "pnpm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@hono/zod-openapi": "^1.1.5",
		"@types/node": "^24.10.1",
		"bumpp": "^10.3.2",
		"tsdown": "^0.17.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"zod": "^4.1.13"
	}
}
